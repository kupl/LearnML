{
  (2,4,(fun x -> x+1)) => 12;
  (1,1000,(fun x -> 0)) => 0;
  (3,6,(fun x -> (x*x)+x)) => 104;
  (1,10,(fun x -> x mod 3)) => 10;
  (5,50,(fun x -> (x/3)+1)) => 452;
  (30,40,(fun x -> (x*x) mod 7)) => 26;
  (20,32,(fun y -> y / (y-15))) => 28;
  (3,10,(fun x -> x * (x/2))) => 178;
  ((-2),0,(fun a -> a+a*a)) => 2;
  ((-10),(-1),(fun a -> a+a+a)) => -165;
}
