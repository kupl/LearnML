{
  (fun x -> x+1);2;4 => 12;
  (fun x -> 0);1;10 => 0;
  (fun x -> (x*x)+x);3;6 => 104;
  (fun x -> x mod 3);1;10 => 10;
  (fun x -> (x/3)+1);5;50 => 452;
  (fun x -> (x*x) mod 7);30;40 => 26;
  (fun y -> y / (y-15));20;32 => 28;
  (fun x -> x * (x/2));3;10 => 178;
  (fun a -> a+a*a);(-2);0 => 2;
  (fun a -> a+a+a);(-10);(-1) => -165;
}
