{
  (0,(fun x->x*3));3 => 3;
  (0,(fun x->x mod 7));3 => 3;
  (0,(fun x->x-3));3 => 3;
  (10,(fun x -> x+1));50 => 60;
  (14,(fun x -> x+10));10 => 150;
  (5, fun x -> (-x));50 => -50;
  (3, (fun x -> x*x));2 => 256;
  (5,(fun x -> x*5));10 => 31250;
  (1,(fun x->x mod 7));27 => 6;
  (5,(fun x -> x/10));1234500 => 12;
}
